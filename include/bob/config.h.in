#ifndef BOB_CONFIG_H 
#define BOB_CONFIG_H

#cmakedefine BOB_VERSION "@BOB_VERSION@"

#cmakedefine BOB_BUILD_DATE "@BOB_BUILD_DATE@"

#cmakedefine BOB_PLATFORM "@BOB_PLATFORM@"

#cmakedefine HAVE_BLITZ_SPECIAL_TYPES 1

#cmakedefine HAVE_BLITZ_TINYVEC2_H 1

#cmakedefine HAVE_FFMPEG 1

#define FFMPEG_VERSION "@FFMPEG_VERSION@"

#cmakedefine HAVE_MATIO 1

#cmakedefine HAVE_MATIO_OLD_COMPLEXSPLIT 1

#cmakedefine HAVE_VLFEAT 1

#cmakedefine HAVE_LIBSVM 1

#cmakedefine HAVE_QT4 1

#cmakedefine HAVE_OPENCV 1

#define OPENCV_VERSION "@OPENCV_VERSION@"

#cmakedefine HAVE_GOOGLE_PERFTOOLS 1

#endif /* BOB_CONFIG_H */
